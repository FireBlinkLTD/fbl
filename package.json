{
  "name": "fbl",
  "version": "0.0.2",
  "description": "",
  "main": "dist/cli.js",
  "scripts": {
    "tsoa": "tsoa",
    "tsc": "tsc",
    "depcheck": "depcheck",
    "rimraf": "rimraf",
    "start": "node dist/src/cli.js",
    "test": "nyc --reporter=html --reporter=text mocha",
    "coverage": "mkdir coverage || nyc --reporter=text-lcov mocha > coverage/coverage.lcov",
    "build": "rimraf ./dist/ && tsc",
    "lint": "tslint -c tslint.json 'src/**/*.ts' 'test/**/*.ts'",
    "prepublish": "npm run build && npm run lint",
    "precommit": "npm run lint"
  },
  "bin": {
    "fbl": "dist/src/cli.js"
  },
  "author": "FireBlink LTD",
  "license": "MIT",
  "dependencies": {
    "@types/commander": "2.12.2",
    "@types/deepmerge": "2.1.0",
    "@types/ejs": "2.6.0",
    "@types/flat": "0.0.28",
    "@types/joi": "13.4.0",    
    "@types/js-yaml": "3.11.2",
    "@types/node": "10.5.2",
    "commander": "2.16.0",    
    "chai-as-promised": "7.1.1",
    "deepmerge": "2.1.1",
    "ejs": "2.6.1",
    "joi": "13.4.0",
    "js-yaml": "3.12.0",
    "reflect-metadata": "0.1.12",
    "typedi": "0.8.0",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "chai": "4.1.2",
    "git-pre-commit": "2.1.4",
    "mocha": "5.2.0",
    "mocha-typescript": "1.1.17",
    "mochawesome": "3.0.2",
    "nyc": "12.0.2",
    "rimraf": "2.6.2",
    "source-map-support": "0.5.6",
    "tmp-promise": "1.0.5",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "exclude": [
      "dist/src/lib/**/index.js",
      "dist/test/**/*.js"
    ]
  },
  "contributors": [
    {
      "name": "Vladyslav Tkachenko",
      "email": "vlad@fireblink.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FireBlinkLTD/fbl.git"
  }
}
